		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.0",
		"rxjs": "^7.8.1",
		"serve": "^14.2.3",
		"tailwind-merge": "^2.5.3",
		"tailwindcss-animate": "^1.0.7",
		"unist-util-visit": "^5.0.0"
	},
