	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeIlluminators/GoodbyeDPI.UI.git"
	},
	"bugs": {
		"url": "https://github.com/CodeIlluminators/GoodbyeDPI.UI/issues"
	},
	"homepage": "https://github.com/CodeIlluminators/GoodbyeDPI.UI/readme",
	"engineStrict": true,
	"engines": {
		"node": ">= 20"
	},
	"lint-staged": {
		"*": [
			"pnpm run clean:write"
		]
	},
	"husky": {
		"hooks": {
			"post-commit": "sh .husky/post-commit.sh"
		}
	},
	"packageManager": "yarn@4.2.2",
	"scripts": {
		"commit": "cz",
		"prepare": "husky",
		"precommit": "lint-staged",
		"boot": "yarn build:back && electron .",
		"dev:front": "next dev frontend",
		"dev:env": "cross-env BROWSER=none next dev",
		"lint:front": "next lint frontend",
		"lint:back": "eslint backend",
		"lint:fix": "prettier -w . && yarn lint:back && yarn lint:front",
		"lint": "prettier -c . && yarn lint:back && yarn lint:front",
		"start": "concurrently -k \"yarn dev:front\" \"wait-on tcp:127.0.0.1:3000 && yarn boot\"",
		"prebuild": "yarn build:front && yarn build:back",
		"build:back": "tsc -p backend && tsc-alias -p ./backend/tsconfig.json",
		"build:front": "next build frontend",
		"build:storybook": "storybook build",
		"build": "yarn prebuild && electron-builder",
		"storybook": "storybook dev -p 6006",
		"postinstall": "electron-builder install-app-deps",
		"format:check": "prettier . --check",
		"clean:check": "run-p sort-imports:check lint",
		"clean:write": "run-p lint:fix sort-imports:write",
		"sort-imports:list": "npx import-sort --list-different \"**/*.{tsx,ts,js,jsx}\"",
		"sort-imports:write": "npx import-sort --write \"**/*.{tsx,ts,js,jsx}\""
	},
